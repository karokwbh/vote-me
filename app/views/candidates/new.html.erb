<h1>Add Candidate</h1>

<%= form_for Candidate.new do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :party %>
<%= form.text_field :party %>
<%= form.label :age %>
<%= form.text_field :age %>
<%= form.label :politics %>
<%= form.text_area :politics %>
<%= form.submit %>
<% end %>